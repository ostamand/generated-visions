{
  "name": "generated-visions",
  "version": "0.1.0",
  "private": false,
  "description": "Generated Visions",
  "author": {
    "name": "ostamand",
    "email": "olivier.st.amand.1@gmail.com"
  },
  "repository": "https://github.com/ostamand/generated-visions",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "npm run electron:dev",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "eslint",
    "electron:dev": "electron-builder install-app-deps && cross-env NODE_ENV=development electron . --no-sandbox",
    "electron:build": "electron-builder install-app-deps && npm run next:build && electron-builder",
    "electron:logo": "npx electron-icon-builder --input=public/logo.png --output=build",
    "clear": "rm -rf dist node_modules && rm -rf ~/.cache/electron-builder && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/archiver": "^7.0.0",
    "archiver": "^7.0.1",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "get-video-dimensions": "^1.0.0",
    "lucide-react": "^0.552.0",
    "next": "^16.0.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-intersection-observer": "^10.0.0",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sharp": "^0.31.1",
    "cross-env": "^10.1.0",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "sass": "^1.93.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0"
  },
  "build": {
    "extends": null,
    "appId": "com.ostamand.generatedVisions",
    "productName": "Generated Visions",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "node_modules",
        "to": "node_modules"
      },
      {
        "from": ".next",
        "to": ".next"
      },
      {
        "from": "public",
        "to": "public"
      },
      {
        "from": "next.config.ts",
        "to": "next.config.ts"
      },
      {
        "from": "postcss.config.mjs",
        "to": "postcss.config.mjs"
      },
      {
        "from": "tsconfig.json",
        "to": "tsconfig.json"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icons/win/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icons/mac/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Graphics",
      "icon": "build/icons/png"
    }
  }
}
